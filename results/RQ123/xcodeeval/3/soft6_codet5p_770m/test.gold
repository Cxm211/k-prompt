#include<stdio.h>  int n,k,q,t,a[105],b[105],c[105];  int compare (const void * a, const void * b) {   return ( *(int*)a - *(int*)b ); }  int main(){  int i,j,tmp,sum,min,max,left_max,left_min,no_el_left=0,no_seen;  scanf("%d%d",&n,&k);  t = n/k;  for(i=0;i<105;i++)   b[i] = 0;  for(i=0;i<n;i++){    scanf("%d",&a[i]);  }  scanf("%d",&q);  for(i=0;i<q;i++){   sum = 0;   for(j=0;j<t;j++){    scanf("%d",&tmp);    sum += a[tmp-1];    b[tmp-1] = 1;   }   if(i==0)    min=max=sum;   else{    if(sum < min)     min = sum;    if(sum > max)     max = sum;   }  }  no_seen=no_el_left=0;  for(i=0;i<n;i++){    if(b[i] == 0){     c[no_el_left++] = a[i];    }else{     no_seen++;    }  }  if(q== 0){   min = 1000000;   max = -1;  }  if(no_seen < t*k && no_el_left >= t){   qsort (c, no_el_left, sizeof(int), compare);   left_max=left_min=0;   for(i=0;i<t;i++)    left_min += c[i];   for(i=no_el_left-1,j=0;j<t;i--,j++)    left_max += c[i];   if(left_min < min)    min = left_min;   if(left_max > max)    max = left_max;     }    printf("%lf %lf",(double)min/t,(double)max/t);  return 0; }
