#include<stdio.h> #include<math.h> char str[200000]; int main() {  int num,start,aim,i,j;  while(~scanf("%d%d%d",&num,&start,&aim)){   getchar();   for(i=1;i<=num;i++){    scanf("%c",&str[i]);   }   if(str[start]==str[aim]||start==aim){    printf("0\n");   }   else{    i=j=start;    while(1){     if(i>1&&str[--i]!=str[start]){      printf("%d\n",abs(i-start));      break;     }     else if(j<num&&str[++j]!=str[start]){      printf("%d\n",abs(j-start));      break;     }    }   }   getchar();  } }
