#include <stdio.h> #include <stdlib.h> #include <string.h> #include <math.h> #include <limits.h>  typedef struct topic{  int a;  int b; } Topic;  int lower_bound(Topic arr[], int n, int x) {     int l = 0;     int h = n;     while (l < h) {         int mid = (l + h) / 2;         if (x <= (arr[mid].a - arr[mid].b)) {             h = mid;         } else {             l = mid + 1;         }     }     return l; }   int cmp(const void* first, const void* second){  return (((Topic*)first)->a - ((Topic*)first)->b) - (((Topic*)second)->a - ((Topic*)second)->b); }  int main(void){  long long count = 0;  int n;  Topic arr[200000];  scanf("%d", &n);   for(int i = 0; i < n; i++){   scanf("%d", &arr[i].a);  }   for(int i = 0; i < n; i++){   scanf("%d", &arr[i].b);  }   qsort(arr, n, sizeof(Topic), cmp);   for(int i = 0; i < n; i++){   int k = lower_bound(arr, n, arr[i].b - arr[i].a + 1);   count += (k < i) ? (n - k - 1) : (n - k);  }   printf("%lld", count / 2);    return 0; }
