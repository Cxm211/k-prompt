#include <stdio.h> #include <stdlib.h> #define N 100005  int cnt[26]; int size=sizeof(cnt[0]); int cmp(const void *a, const void *b) {  return *(int*)b-*(int*)a; } int min(int a, int b) {  return a<b? a : b; } char s[N];  int main(void) {  int n, k, i;  long long ans, t;    scanf("%d%d%*c", &n, &k);  gets(s);  for (i=0; i<n; ++i)   ++cnt[s[i]-'A'];  qsort(cnt, 26, size, cmp);  ans = 0;  for (i=0; k; ++i) {   t = min(k,cnt[i]);   ans += t*t;   k -= t;  }  printf("%I64d\n", ans);   return 0; }
