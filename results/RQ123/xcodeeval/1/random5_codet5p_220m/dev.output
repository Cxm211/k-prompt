/* practice with Dukkha */ #include <stdio.h>  #define N 1000 #define B 10  int qq[N], cnt, x, y;  int query() {  int a = 0;   if (cnt) {   int i;    printf("? %d", cnt);   for (i = 0; i < cnt; i++)    printf(" %d", qq[i]);   printf("\n");   fflush(stdout);   scanf("%d", &a);  }  return a!= (cnt % 2 == 0? 0 : x); }  int solve(int n) {  int lower = 0, upper = n;   while (upper - lower > 1) {   cnt = (lower + upper) / 2;   if (query())    upper = cnt;   else    lower = cnt;  }  return qq[upper - 1]; }  int main() {  static char diff[B];  int n, b, i, p1, p2, tmp;   scanf("%d%d%d", &n, &x, &y);  for (b = 0; b < B; b++) {   cnt = 0;   for (i = 1; i <= n; i++)    if (i & 1 << b)     qq[cnt++] = i;   diff[b] = query();  }  printf("\n");  for (b = 0; b < B; b++)   printf("%d ", diff[b]);  printf("\n");  fflush(stdout);  cnt = 0;  for (b = 0; b < B; b++)   if (diff[b]) {    for (i = 1; i <= n; i++)     if (i & 1 << b)      qq[cnt++] = i;    break;   }  p1 = solve(cnt);  p2 = p1;  for (b = 0; b < B; b++)   if (diff[b])    p2 ^= 1 << b;  if (p1 > p2)   tmp = p1, p1 = p2, p2 = tmp;  printf("! %d %d\n", p1, p2);  fflush(stdout);  return 0; }
