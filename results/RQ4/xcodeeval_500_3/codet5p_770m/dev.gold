#include <stdio.h>  #include <string.h>  void swap(int* a, int* b);  void max_heapify(int arr[], int start, int end);  void heap_sort(int arr[], int len);  int main()  {   int t, i;   scanf("%d", &t);   while (t--)   {    int len;    scanf("%d", &len);    int list[len];    for (i = 0; i < len; i++)     scanf("%d", &list[i]);    heap_sort(list, len);    int tem = list[0];    for (i = 0; i < len - 1; i++)     if (list[i + 1] - list[i] > tem)      tem = list[i + 1] - list[i];    printf("%d\n", tem);   }   return 0;  }  void swap(int* a, int* b) {   int temp = *b;   *b = *a;   *a = temp;  }  void max_heapify(int arr[], int start, int end) {   int dad = start;   int son = dad * 2 + 1;   while (son <= end) {    if (son + 1 <= end && arr[son] < arr[son + 1])     son++;    if (arr[dad] > arr[son])     return;    else {     swap(&arr[dad], &arr[son]);     dad = son;     son = dad * 2 + 1;    }   }  }  void heap_sort(int arr[], int len) {   int i;   for (i = len / 2 - 1; i >= 0; i--)    max_heapify(arr, i, len - 1);   for (i = len - 1; i > 0; i--) {    swap(&arr[0], &arr[i]);    max_heapify(arr, 0, i - 1);   }  }
