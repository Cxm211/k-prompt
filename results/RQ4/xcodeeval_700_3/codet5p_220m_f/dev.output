#include <stdio.h>  int main()  {   unsigned long long t;   scanf("%llu", &t);   while(t--)   {    unsigned long long k;    scanf("%llu", &k);    char arr[k+5];     unsigned long long i, count=0, star_ref=0;    unsigned long long steps=0;    unsigned long long start=-1, end=-1;    scanf("%s", arr);    for (i=0; i<k; i++)    {     if (arr[i] == '*')     {      star_ref++;      if (star_ref==1)      {       start=i;      }      if (start>=0)      {       end=i;      }      }     }    //printf("Start: %llu, End: %llu\n", start, end);    for (i=start; i<=end; i++) //cuma perlu ngecek dari start * ke end *    {     if (arr[i] == '*')     {      count++;     }     else     {      if (count < star_ref-count)      {       steps+=count;      }      else      {       steps+=star_ref-count;      }     }    }    printf("%llu\n", steps);   }  }
