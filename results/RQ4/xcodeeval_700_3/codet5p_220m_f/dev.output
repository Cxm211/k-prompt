#include <stdio.h>  #include <string.h>  void swap(int* a, int *b)  {   int tem;   tem = *a;   *a = *b;   *b = tem;  }  void HeapAdjust(int *a, int s, int m)  {   int rc, j;   rc = a[s];   for (j = 2 * s; j <= m; j = j * 2)   {    if (j < m&& a[j] < a[j + 1]) j++;    if (rc > a[j]) break;    a[s] = a[j]; s = j;   }   a[s] = rc;  }  void HeapSort(int *a, int n)  {   int temp, i, j;   for (i = n / 2; i > 0; i--)   {    HeapAdjust(a, i, n);   }   for (i = n; i > 0; i--)   {    temp = a[1];    a[1] = a[i];    a[i] = temp;    HeapAdjust(a, 1, i - 1);   }   for (i = 0; i < n; i++)    swap(&a[i], &a[i + 1]);  }  int main()  {   int t, i;   scanf("%d", &t);   while (t--)   {    int len;    scanf("%d", &len);    int list[1000];    for (i = 0; i < len; i++)     scanf("%d", &list[i]);    getchar();    HeapSort(list, len - 1);    int tem = list[0];    for (i = 0; i < len - 1; i++)     if (list[i + 1] - list[i] > tem)      tem = list[i + 1] - list[i];    printf("%d\n", tem);   }   return 0;  }
