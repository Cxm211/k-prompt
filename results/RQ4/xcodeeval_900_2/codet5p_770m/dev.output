#include <stdio.h> #include <stdlib.h>  /*  * Codeforces 1234A */  int progress(int xo[],int count) {  int i,max=0,min=0,lack,ans;  min = xo[0];  for(i=0;i<count;i++) {   // find the max   if(xo[i]>=max)    max = xo[i];   // find the min   if(xo[i]<=min)    min = xo[i];  }  lack = max - min;  for(i=0;i<count;i++) {   if(xo[i]!= max)    xo[i] += lack;  }  i = 0;  ans = 0;  // find the difference value  while(i < count) {   if(xo[i] > max) {    ans = 1;    break;   }   i++;  }  if(ans == 1)   printf("NO\n");  else   printf("YES\n"); }  int main () {  int t,i,n,j;  scanf("%d",&t);  for(i=0;i<t;i++) {   scanf("%d",&n);   int numrray[n];   for(j=0;j<n;j++)    scanf("%d",&numrray[j]);   progress(numrray,n);  }  return 0; }
