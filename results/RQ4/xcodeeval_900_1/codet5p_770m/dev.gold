#include <stdio.h> #include <stdbool.h>  #define psize 200000 bool primes[psize];  int main() {     long long int n;     scanf("%I64d", &n);          if(n < 3) {         printf("-1");         return 0;     }          long long int mul = 1;     //reduce n to a prime     for(int i = 0; i < psize; i++)         primes[i] = true;              for(int i = 2; i < psize; i++)         for(int j = 2*i; j < psize; j+=i)             primes[j] = false;          long long int lastPrime = 1;     for(long long int i = 2; i < psize; i++) {         if(i > n)             break;         while(primes[i] && (n % i) == 0) {             n /= i;             mul *= i;             lastPrime = i;         }     }          if(n>=psize)         lastPrime = n;     else         mul = mul/lastPrime;          if (lastPrime == 2) {         mul /= 2;         lastPrime *= 2;         printf("%I64d %I64d", 5*mul, 3*mul);         return 0;     }          long long int nplus = (lastPrime*lastPrime+1)/2;     printf("%I64d %I64d", (nplus-1)*mul, nplus*mul);     return 0; }
