#include <stdio.h> #include <stdlib.h>  int result(long A[],long n) {     int i,max=A[0],k=-1;     for (i=1;i<n;i++)     {         if (A[i]!=max) k=0;         if (A[i]>max) max=A[i];     }     if (k==-1) return k;     if (A[0]==max && A[0]>A[1]) return 0+1;     if (A[n-1]==max && A[n-1]>A[n-2]) return n;     for (i=1;i<n-1;i++)     {         if ((A[i]==max) && (A[i]>A[i-1]||A[i]>A[i+1]))             return i+1;     }  }  int main() {     int t;     scanf("%d%*c",&t);     long i,n,j;     long *A;     for (i=0;i<t;i++)     {         scanf("%d%*c",&n);         A=(long)malloc(sizeof(long)*n);         for (j=0;j<n;j++)             scanf("%d%*c",&A[j]);             printf("%d\n",result(A,n));         free(A);     }      return 0; }
